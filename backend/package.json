{
  "name": "siwe-workshop-backend",
  "version": "1.0.0",
  "author": "https://github.com/johannafransn",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc -p tsconfig.json",
    "start": "ts-node -r tsconfig-paths/register build/index.js",
    "rebuild": "rm -rf build/ && tsc -p tsconfig.json",
    "reinstall": "rm -rf node_modules/ package-lock.json && npm i",
    "db:generate": "prisma generate",
    "db:dev": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:status": "prisma migrate status",
    "db:push": "prisma db push",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@fastify/accepts": "4.3.0",
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "8.5.0",
    "@fastify/formbody": "7.4.0",
    "@fastify/helmet": "11.1.1",
    "@fastify/rate-limit": "9.0.1",
    "@fastify/session": "^10.7.0",
    "@prisma/client": "^5.7.1",
    "cors": "2.8.5",
    "fastify": "4.25.2",
    "jsonwebtoken": "^9.0.2",
    "siwe": "^2.1.4"
  },
  "peerDependencies": {
    "@sinclair/typebox": "^0.31"
  },
  "devDependencies": {
    "@fastify/type-provider-typebox": "3.5.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^18.11.18",
    "@types/nodemon": "1.19.6",
    "nodemon": "3.0.2",
    "prettier": "3.1.1",
    "prisma": "^5.7.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.0.4"
  },
  "ts-node": {
    "require": [
      "tsconfig-paths/register"
    ]
  }
}
